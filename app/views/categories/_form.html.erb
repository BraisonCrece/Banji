<%= form_with model: category, class: 'formulario' do |form| %>
  <% category.errors.full_messages.each do |error|  %>
      <p class="error_message"><%= error %></p>
  <% end %>

  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
